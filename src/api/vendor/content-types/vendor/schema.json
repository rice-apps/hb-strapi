{
  "kind": "collectionType",
  "collectionName": "vendors",
  "info": {
    "singularName": "vendor",
    "pluralName": "vendors",
    "displayName": "Vendor",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "import-export-entries": {
      "alias": {
        "Company": "name",
        "NM Category 1": "categories",
        "NM Category 2": "categories",
        "NM Description": "shortDesc",
        "Program City/State": "location",
        "Program Phone": "phone",
        "Program Email": "email"
      }
    }
  },
  "attributes": {
    "name": {
      "type": "string",
      "unique": true
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "mappedBy": "vendors"
    },
    "primaryImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "additionalImages": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "shortDesc": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "email": {
      "type": "email"
    },
    "location": {
      "type": "string"
    },
    "phone": {
      "type": "string"
    }
  }
}
